    "def z_score(x):\n",
    "    n = x[0].shape[0]\n",
    "    for i in range(n):\n",
    "        mean = np.mean(x[:,i])\n",
    "        std = np.std(x[:,i])\n",
    "        x[:,i] = (x[:,i]-mean)/std\n",
    "    return x"
   ]
  }
